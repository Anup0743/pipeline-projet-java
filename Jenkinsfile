pipeline
{
    agent any
    stages
    {
        stage('compile')
        {
            stape
            {
                echo "complieing the code"
                bat 'javac Test.java'

            }
        }
        stage('build')
        {
            stape
            {
                echo "build the code"
                java Test 

            }
        }
        stage('deploy')
        {
            stape
            {
                echo "deploy the code"
               

            }
        }
    }
}